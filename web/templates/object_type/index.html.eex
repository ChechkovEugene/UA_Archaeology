<h2>Типи об'єктів</h2>

<table class="table">
  <thead>
    <tr>
      <th>Тип об'єктів</th>

      <th></th>
    </tr>
  </thead>
  <tbody>
<%= for object_type <- @object_types do %>
    <tr>
      <td><%= object_type.name %></td>

      <td class="text-right">
        <%= link "Переглянути", to: user_object_type_path(@conn, :show, @user, object_type),
          class: "btn btn-default btn-xs" %>
        <%= link "Редагувати", to: user_object_type_path(@conn, :edit, @user, object_type),
          class: "btn btn-default btn-xs" %>
        <%= link "Видалити", to: user_object_type_path(@conn, :delete, @user,
          object_type), method: :delete, data: [confirm: "Are you sure?"],
            class: "btn btn-danger btn-xs" %>
      </td>
    </tr>
<% end %>
  </tbody>
</table>

<%= link "Новий тип об'єктів", to: user_object_type_path(@conn, :new, @user) %>
